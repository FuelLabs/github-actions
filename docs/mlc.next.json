{
  "aliveStatusCodes": [200, 206, 405],
  "ignorePatterns": [
    {
      "pattern": "^http://localhost:"
    },
    {
      "pattern": "^@repository"
    },
    {
      "pattern": "\\.zip$"
    },
    {
      "pattern": "\\.png$"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/docs/(?!.*\\.mdx$)",
      "replacement": "$&.mdx"
    }
  ]
}

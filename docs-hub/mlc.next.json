{
  "aliveStatusCodes": [200, 206, 405],
  "ignorePatterns": [
    {
      "pattern": "localhost:"
    },
    {
      "pattern": "^http://[1-9]"
    },
    {
      "pattern": "crates\\.io"
    },
    {
      "pattern": "infura\\.io"
    },
    {
      "pattern": "-indexer.fuel.network"
    },
    {
      "pattern": "fuellabs.github.io/block-explorer-v2"
    },
    {
      "pattern": "adobe\\.com"
    },
    {
      "pattern": "scripts.sil.org"
    },
    {
      "pattern": "^@repository"
    },
    {
      "pattern": "\\.zip$"
    },
    {
      "pattern": "\\.png$"
    },
    {
      "pattern": "\\b(?<!d)(http[s]?://)?(www\\.)?x\\.com\\b"
    },
    {
      "pattern": "twitter.com"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^(/)(.*)",
      "replacement": "{{VERCEL_PREVIEW}}/$2"
    }
  ]
}
